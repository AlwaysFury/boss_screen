<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boss.task.dao.PayoutInfoDao">

    <select id="selectMaxTime" resultType="long">
        select max(s.payout_time) from tb_order as s where shop_id = #{shopId}
    </select>

</mapper>
